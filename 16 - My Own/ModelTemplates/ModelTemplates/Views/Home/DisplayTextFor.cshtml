@model ModelTemplates.Models.Person

@{
    ViewBag.Title = "DisplayTextFor";
}

<h2>Person</h2>

<div class="field">
    <label>Name:</label>
    @Html.DisplayTextFor(m => m.FirstName)
    @Html.DisplayTextFor(m => m.LastName)
</div>

<div class="field">
    <label>Approved:</label>
    @Html.DisplayTextFor(m => m.IsApproved)
</div>
